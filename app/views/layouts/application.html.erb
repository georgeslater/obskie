<!DOCTYPE html>
<html>
<head>
  <title>Obscure Albums</title>
  <%= favicon_link_tag 'favicon.ico' %>
  <link href="//netdna.bootstrapcdn.com/bootstrap/<%= bootstrap_version %>/css/bootstrap.min.css" rel="stylesheet">
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= render "layouts/head" %>
   <script src="//netdna.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>
</head>
<body>
<%= render "layouts/header" %>
<% if alert %>
  <p class="alert alert-danger"><%= alert %></p>
<% end %>

<%= yield %>
<!--<%= render "layouts/footer" unless current_page?(albums_path) || current_page?(root_path)%>-->
</body>
</html>
